{
	"extends": "prettier",
	"rules": {
		"block-scoped-var": 2,
		"camelcase": [2, { "properties": "never" }],
		"dot-notation": 2,
		"eqeqeq": [2, "always"],
		"guard-for-in": 2,
		"no-empty": [2, { "allowEmptyCatch": true }],
		"no-loop-func": 2,
		"no-script-url": 2,
		"no-shadow": 2,
		"quote-props": [2, "as-needed", { "keywords": true }]
	}
}
