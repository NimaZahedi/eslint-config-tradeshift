{
	"extends": "pretty-standard",
	"rules": {
		"block-scoped-var": "error",
		"camelcase": ["error", { "properties": "never" }],
		"dot-notation": "error",
		"eqeqeq": ["error", "always"],
		"guard-for-in": "error",
		"no-empty": ["error", { "allowEmptyCatch": true }],
		"no-loop-func": "error",
		"no-script-url": "error",
		"no-shadow": "error",
		"quote-props": ["error", "as-needed", { "keywords": true }]
	}
}
